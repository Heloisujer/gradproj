.login-header {
  width: 100%;
  height: 100vh;
}
.login-header .logobg-img {
  width: 100%;
  min-width: 1200px;
  height: 100%;
}
.login-header .logobg-img .login-logo {
  float: left;
  margin-top: 40px;
  margin-left: 20px;
}
.login-header .logobg-img .login-title {
  margin-top: 64px;
  padding-left: 26px;
  font-size: 32px;
  color: #007dbf;
  float: left;
}
.login-header .logobg-img .login-content {
  width: 20%;
  min-width: 300px;
  clear: both;
  position: absolute;
  top: 38%;
  margin-left: 7%;
}
.login-header .logobg-img .login-content .identify-code {
  position: absolute;
  top: 4%;
  margin-left: 105%;
}
.login-header .logobg-img .login-content .el-button {
  width: 60px;
  height: 30px;
  line-height: 2px;
  margin-left: 20px;
  margin-right: 30px;
}
.login-header .logobg-img .login-content .el-button span {
  margin-left: -4px;
}
.login-header .logobg-img .login-content .el-button--default {
  margin: 0;
}
